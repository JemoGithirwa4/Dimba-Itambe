            <img src="/images/logo-2.png" alt="Team Logo">
            <div class="stat">
              <span class="value">282</span>
              <span>Appearances</span>
            </div>
            <div class="stat">
              <span class="value">175</span>
              <span>Goals</span>
            </div>
